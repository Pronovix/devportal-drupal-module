{
    "name": "drupal/devportal",
    "type": "drupal-module",
    "description": "Common devportal functionality",
    "license": "GPL-2.0+",
    "require": {
        "php": "^7.1",
        "drupal/core": "^8.7",
        "drupal/swagger_ui_formatter": "^2.3",
        "drupal/views_accordion": "^1.2",
        "erusev/parsedown": "^1.6.3",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "behat/mink": "dev-master#153e4bb",
        "drupal/drupal-extension": "^3.4.0",
        "instaclick/php-webdriver": "^1.4.5",
        "phpunit/phpunit": "^6.5",
        "webflo/drupal-core-require-dev": "^8.7"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\devportal\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\devportal\\": "./tests/"
        }
    }
}
